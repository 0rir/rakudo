
$(SETTING_@ucspec@_MOAR): $(PERL6_MOAR) $(PERL6_B_MOAR) $(SETTING_MOAR) $(M_CORE_@ucspec@_SOURCES)
	$(M_NQP) $(M_GEN_CAT) -f @template(moar_core_sources)@ > $(M_BUILD_DIR)/CORE.@lcspec@.setting
	@echo "The following step can take a long time, please be patient."
	$(M_RUN_PERL6) --setting=NULL.@lcspec@ --ll-exception --optimize=3 --target=mbc --stagestats --output=$(SETTING_@ucspec@_MOAR) $(M_BUILD_DIR)/CORE.@lcspec@.setting
