spectest_checkout : t/spec
t/spec :
	git clone https://github.com/perl6/roast t/spec

spectest_update :
	-cd t/spec && git pull --ff-only

t/localtest.data:
	$(TEST_F) $@

realclean: clean
	$(RM_F) Makefile config.status MANIFEST
	$(RM_RF) lib/.precomp/ t/04-nativecall/.precomp/

distclean: realclean
